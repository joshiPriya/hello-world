node {

stage('delet ws')
	{
	cleanWs()	
	}
	
	
    
 stage('Git checkout'){
	git 'https://github.com/joshiPriya/hello-world.git'
}

stage('compile-package')
{
    def mvnhome = tool name: 'maven3', type: 'maven'
    sh "${mvnhome}/bin/mvn deploy:deploy-file -DgroupId=com.somecompany -DartifactId=project -Dversion=1.0.0 -DgeneratePom=true -Dpackaging=jar -DrepositoryId=nexus -Durl=http://10.100.90.206:8081/repository/maven-releases/ -Dfile=target/project-1.0.0.jar"
   

}
}
